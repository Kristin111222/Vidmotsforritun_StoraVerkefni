<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane prefHeight="290.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="fxAukaSpurning" prefHeight="56.0" prefWidth="227.0" text="Hvað stendur CSS fyrir?" />
                  <Label fx:id="fxTimer" text="Timer" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="fxSvaraAukaSpurningu" onAction="#onSvaraAukaSpurningu" prefHeight="42.0" prefWidth="171.0" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextArea fx:id="fxFeedbackAukaSpurning" prefHeight="200.0" prefWidth="200.0" />
               </children></HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" />
         </children>
      </VBox>
</content>
<ButtonType fx:id="fxILagi" buttonData="OK_DONE" text="Í lagi" />
<ButtonType fx:id="fxHaettaVid" buttonData="CANCEL_CLOSE" text="Hætta við" />
</DialogPane>
